	"devDependencies": {
		"@colyseus/auth": "^0.16.4",
		"@colyseus/core": "^0.16.8",
		"@colyseus/loadtest": "^0.16.1",
		"@colyseus/redis-driver": "^0.16.1",
		"@colyseus/redis-presence": "^0.16.1",
		"@colyseus/schema": "^3.0.16",
		"@colyseus/testing": "^0.16.3",
		"@colyseus/uwebsockets-transport": "^0.16.4",
		"@colyseus/ws-transport": "^0.16.3",
		"@types/express": "^5.0.0",
		"@types/mocha": "^10.0.10",
		"@types/three": "^0.174.0",
		"mocha": "^10.8.2",
